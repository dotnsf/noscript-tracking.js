<html>
<head>
<title>index</title>
<style>
*{ margin:0; padding:0 }
html,body { width:100%; height:100% }
p{
  width:10px;
  height:10px;
  display:inline-block;
  border-right:1px solid #666;
  border-bottom:1px solid #666
}
</style>
</head>
<body>
<% for( var i = 0; i < 50; i ++ ){ %>
  <div>
<%   for( var j = 0; j < 50; j ++ ){ %>
    <p id="c<%= i %>x<%= j %>"></p>
<%   } %>
  </div>
<% } %>
<style>
<% for( var i = 0; i < 50; i ++ ){ %>
<%   for( var j = 0; j < 50; j ++ ){ %>
#c<%= i %>x<%= j %>:hover{background:url("c<%= i %>x<%= j %>.png")}
<%   } %>
<% } %>
</style>
</body>
</html>
